{
  "name": "simplon-drivers-app",
  "main": "expo-router/entry",
  "version": "1.0.4",
  "scripts": {
    "start": "expo start --clear",
    "start:expo-go": "EXPO_USE_DEV_SERVER=true expo start --clear --dev-client=false",
    "start:tunnel": "expo start --clear --tunnel --dev-client=false",
    "start:lan": "expo start --clear --lan --dev-client=false",
    "start:localhost": "expo start --clear --localhost --dev-client=false",
    "start:development": "EXPO_USE_DEV_SERVER=true expo start --clear --dev-client=false",
    "dev": "EXPO_USE_DEV_SERVER=true expo start --clear --dev-client=false",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo start --android --dev-client=false",
    "ios": "expo start --ios --dev-client=false", 
    "android:build": "expo run:android",
    "ios:build": "expo run:ios",
    "ios:clean": "expo prebuild --platform ios --clean && expo run:ios",
    "android:clean": "expo prebuild --platform android --clean && expo run:android",
    "prebuild": "expo prebuild",
    "prebuild:clean": "expo prebuild --clean",
    "web": "expo start --web",
    "lint": "expo lint",
    "version:bump": "node version-bump.js",
    "version:patch": "node version-bump.js patch",
    "version:minor": "node version-bump.js minor",
    "version:major": "node version-bump.js major",
    "prebuild:dev": "npm run version:bump",
    "build:dev": "npm run prebuild:dev && eas build --platform all --profile development",
    "build:dev:android": "npm run prebuild:dev && eas build --platform android --profile development",
    "build:dev:ios": "npm run prebuild:dev && eas build --platform ios --profile development",
    "build:auto": "node build-auto.js",
    "build:auto:android": "node build-auto.js android",
    "build:auto:ios": "node build-auto.js ios",
    "build:development": "eas build --platform all --profile development",
    "build:apk": "eas build --platform android --profile apk",
    "build:preview": "eas build --platform android --profile preview",
    "build:production:android": "eas build --platform android --profile production",
    "build:production:ios": "eas build --platform ios --profile production",
    "submit:android": "eas submit --platform android --profile production",
    "submit:ios": "eas submit --platform ios --profile production",
    "publish:android": "npm run build:production:android && npm run submit:android",
    "publish:ios": "npm run build:production:ios && npm run submit:ios",
    "generate:icons": "node generate-icons-node.js",
    "build:script": "./build-dev.sh",
    "build:script:windows": "build-dev.bat",
    "maps:enable": "npm run maps:backup && npm run maps:activate",
    "maps:disable": "npm run maps:backup && npm run maps:restore-simple",
    "maps:backup": "cp app/admin/create-mission.tsx app/admin/create-mission-backup.tsx 2>/dev/null || true",
    "maps:activate": "mv app/admin/create-mission-simple.tsx app/admin/create-mission-simple-backup.tsx 2>/dev/null || true && mv app/admin/create-mission-maps.tsx app/admin/create-mission.tsx 2>/dev/null || true",
    "maps:restore-simple": "mv app/admin/create-mission.tsx app/admin/create-mission-maps.tsx 2>/dev/null || true && mv app/admin/create-mission-simple.tsx app/admin/create-mission.tsx 2>/dev/null || true"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-navigation/bottom-tabs": "^7.3.10",
    "@react-navigation/elements": "^2.3.8",
    "@react-navigation/native": "^7.1.6",
    "@types/date-fns": "^2.5.3",
    "@types/react-native-maps": "^0.24.1",
    "date-fns": "^4.1.0",
    "expo": "~53.0.20",
    "expo-blur": "~14.1.5",
    "expo-constants": "~17.1.7",
    "expo-crypto": "~14.0.1",
    "expo-font": "~13.3.2",
    "expo-haptics": "~14.1.4",
    "expo-image": "~2.4.0",
    "expo-linking": "~7.1.7",
    "expo-location": "^18.1.6",
    "expo-maps": "~0.11.0",
    "expo-print": "^14.1.4",
    "expo-router": "~5.1.4",
    "expo-secure-store": "^14.2.3",
    "expo-sharing": "^13.1.5",
    "expo-splash-screen": "~0.30.10",
    "expo-sqlite": "~15.0.2",
    "expo-status-bar": "~2.2.3",
    "expo-symbols": "~0.4.5",
    "expo-system-ui": "~5.0.10",
    "expo-web-browser": "~14.2.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.5",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-maps": "1.20.1",
    "react-native-paper": "^5.14.5",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-web": "~0.20.0",
    "react-native-webview": "13.13.5",
    "sharp": "^0.34.3",
    "expo-dev-client": "~5.2.4",
    "expo-updates": "~0.28.17"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/react": "~19.0.10",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~9.2.0",
    "tsx": "^4.20.3",
    "typescript": "~5.8.3"
  },
  "private": true
}
